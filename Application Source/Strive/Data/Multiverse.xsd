            <xs:field xpath="mstns:PlayerID" />
        </xs:keyref>
        <xs:key name="MobilePK">
            <xs:selector xpath=".//mstns:Mobile" />
            <xs:field xpath="mstns:SpawnID" />
        </xs:key>
        <xs:keyref name="AreaMobile" refer="AreaPK">
            <xs:selector xpath=".//mstns:Mobile" />
            <xs:field xpath="mstns:AreaID" />
        </xs:keyref>
        <xs:keyref name="MobileMobilePossesableByPlayer" refer="MobilePK">
            <xs:selector xpath=".//mstns:MobilePossesableByPlayer" />
            <xs:field xpath="mstns:SpawnID" />
        </xs:keyref>
        <xs:keyref name="MobileInventory" refer="MobilePK">
            <xs:selector xpath=".//mstns:Inventory" />
            <xs:field xpath="mstns:SpawnID" />
        </xs:keyref>
        <xs:keyref name="AreaQuaffable" refer="AreaPK">
            <xs:selector xpath=".//mstns:Quaffable" />
            <xs:field xpath="mstns:AreaID" />
        </xs:keyref>
        <xs:keyref name="AreaJunk" refer="AreaPK">
            <xs:selector xpath=".//mstns:Junk" />
            <xs:field xpath="mstns:AreaID" />
        </xs:keyref>
        <xs:keyref name="WieldableInventory" refer="WieldablePK">
            <xs:selector xpath=".//mstns:Inventory" />
            <xs:field xpath="mstns:SpawnID" />
        </xs:keyref>
    </xs:element>
</xs:schema>
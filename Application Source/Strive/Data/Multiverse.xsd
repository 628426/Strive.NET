                            <xs:element name="Hitroll" type="xs:int" />
                            <xs:element name="DamageTypeID" type="xs:int" />
                            <xs:element name="SpawnID" type="xs:int" minOccurs="0" />
                            <xs:element name="X" type="xs:int" minOccurs="0" />
                            <xs:element name="Y" type="xs:int" minOccurs="0" />
                            <xs:element name="Z" type="xs:int" minOccurs="0" />
                            <xs:element name="HeadingX" type="xs:int" minOccurs="0" />
                            <xs:element name="HeadingY" type="xs:int" minOccurs="0" />
                            <xs:element name="HeadingZ" type="xs:int" minOccurs="0" />
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element name="MobilePossesableByPlayer">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="MobileID" type="xs:int" />
                            <xs:element name="PlayerID" type="xs:int" />
                            <xs:element name="SpawnID" type="xs:int" minOccurs="0" />
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
                <xs:element name="Inventory">
                    <xs:complexType>
                        <xs:sequence>
                            <xs:element name="PhysicalObjectID" type="xs:int" />
                            <xs:element name="ItemID" type="xs:int" />
                            <xs:element name="SpawnID" type="xs:int" minOccurs="0" />
                        </xs:sequence>
                    </xs:complexType>
                </xs:element>
            </xs:choice>
        </xs:complexType>
        <xs:key name="WorldPK">
            <xs:selector xpath=".//mstns:World" />
            <xs:field xpath="mstns:WorldID" />
        </xs:key>
        <xs:key name="AreaPK">
            <xs:selector xpath=".//mstns:Area" />
            <xs:field xpath="mstns:AreaID" />
        </xs:key>
        <xs:keyref name="WorldArea" refer="WorldPK">
            <xs:selector xpath=".//mstns:Area" />
            <xs:field xpath="mstns:WorldID" />
        </xs:keyref>
        <xs:key name="TerrainPK">
            <xs:selector xpath=".//mstns:Terrain" />
            <xs:field xpath="mstns:SpawnID" />
        </xs:key>
        <xs:keyref name="AreaTerrain" refer="AreaPK">
            <xs:selector xpath=".//mstns:Terrain" />
            <xs:field xpath="mstns:AreaID" />
        </xs:keyref>
        <xs:key name="WieldablePK">
            <xs:selector xpath=".//mstns:Wieldable" />
            <xs:field xpath="mstns:SpawnID" />
        </xs:key>
        <xs:keyref name="AreaWieldable" refer="AreaPK">
